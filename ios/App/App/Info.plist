	<string>Dash accesses your photo library to attach photos to tasks and events.</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>Dash saves photos to your photo library.</string>
	<key>NSSiriUsageDescription</key>
	<string>Dash uses Siri to help you quickly add tasks and events.</string>
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleURLName</key>
			<string>io.matj.dash</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>dash</string>
			</array>
		</dict>
	</array>
